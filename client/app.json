{
  "expo": {
    "name": "BreakFirst",
    "slug": "BreakFirst",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/breakfirst.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/breakfirst.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.atwalsultan.breakfirst",
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/breakfirst.png"
        }
      ]
    ]
  }
}
